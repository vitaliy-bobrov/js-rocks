{
  "name": "js-rocks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:ci": "ng run js-rocks:app-shell:production",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng deploy",
    "pwa-asset": "pwa-asset-generator -c -m ./src/manifest.webmanifest -i ./src/index.html -q 90 -b '#303030' logo.png ./src/assets/icon",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.2",
    "@angular/cdk": "~11.2.2",
    "@angular/common": "~11.2.2",
    "@angular/compiler": "~11.2.2",
    "@angular/core": "~11.2.2",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~11.2.2",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "~11.2.2",
    "@angular/platform-browser-dynamic": "~11.2.2",
    "@angular/platform-server": "~11.2.2",
    "@angular/router": "~11.2.2",
    "@angular/service-worker": "~11.2.2",
    "angular-google-tag-manager": "^1.1.4",
    "core-js": "3.6.5",
    "firebase": "^7.15.4",
    "nanoid": "^3.1.10",
    "ng-sortgrid": "^4.0.0",
    "ngx-loadable": "^3.0.0-next.12",
    "rxjs": "~6.6.0",
    "standardized-audio-context": "^25.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1102.2",
    "@angular-devkit/build-angular": "~0.1102.2",
    "@angular/cli": "~11.2.2",
    "@angular/compiler-cli": "~11.2.2",
    "@angular/language-service": "~11.2.2",
    "@types/jest": "26.0.13",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.0.14",
    "axe-core": "^4.0.1",
    "codelyzer": "^6.0.0",
    "cssnano": "^4.1.10",
    "firebase-tools": "^8.4.3",
    "fuzzy": "^0.1.3",
    "husky": "4.3.0",
    "inquirer": "^7.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^26.1.0",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "10.3.0",
    "prettier": "2.1.1",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.1.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@audio/(.*)": "<rootDir>/src/app/audio/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}

{
  "name": "js-rocks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:ci": "ng run js-rocks:app-shell:production",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng deploy",
    "pwa-asset": "pwa-asset-generator -c -m ./src/manifest.webmanifest -i ./src/index.html -q 90 -b '#303030' logo.png ./src/assets/icon",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/fire": "^7.0.4",
    "@angular/forms": "^17.0.5",
    "@angular/material": "^16.2.0",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.5",
    "@angular/platform-server": "^17.0.5",
    "@angular/router": "^17.0.5",
    "@angular/service-worker": "^17.0.5",
    "angular-google-tag-manager": "^1.1.4",
    "core-js": "3.6.5",
    "firebase": "^10.7.0",
    "nanoid": "^3.1.10",
    "ng-sortgrid": "^4.0.0",
    "ngx-loadable": "^2.0.1",
    "rxjs": "~6.6.0",
    "standardized-audio-context": "^25.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.5",
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular/cli": "^17.0.5",
    "@angular/compiler-cli": "^17.0.5",
    "@angular/language-service": "^17.0.5",
    "@types/jest": "26.0.13",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.0.14",
    "axe-core": "^4.0.1",
    "codelyzer": "^0.0.28",
    "cssnano": "^6.0.1",
    "firebase-tools": "^12.9.1",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.8",
    "inquirer": "^7.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "8.3.1",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "protractor": "^3.3.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@audio/(.*)": "<rootDir>/src/app/audio/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}

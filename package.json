{
  "name": "js-rocks",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier --write \"**/*.{js,json,scss,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "ng deploy",
    "pwa-asset": "pwa-asset-generator -c -m ./src/manifest.webmanifest -i ./src/index.html -q 90 -b '#303030' logo.png ./src/assets/icon",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0-rc.7",
    "@angular/cdk": "~9.0.0-rc.6",
    "@angular/common": "~9.0.0-rc.7",
    "@angular/compiler": "~9.0.0-rc.7",
    "@angular/core": "~9.0.0-rc.7",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~9.0.0-rc.7",
    "@angular/material": "^9.0.0-rc.6",
    "@angular/platform-browser": "~9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.7",
    "@angular/router": "~9.0.0-rc.7",
    "@angular/service-worker": "~9.0.0-rc.7",
    "angular-google-tag-manager": "^1.0.1",
    "core-js": "3.6.4",
    "firebase": "^7.2.2",
    "nanoid": "^2.1.6",
    "ngx-loadable": "^3.0.0-next.12",
    "rxjs": "~6.5.3",
    "standardized-audio-context": "^23.1.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.15",
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular/cli": "~9.0.0-rc.7",
    "@angular/compiler-cli": "~9.0.0-rc.7",
    "@angular/language-service": "~9.0.0-rc.7",
    "@types/jest": "24.9.1",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.1.2",
    "axe-core": "^3.4.0",
    "codelyzer": "^5.1.2",
    "cssnano": "^4.1.10",
    "firebase-tools": "^7.6.1",
    "fuzzy": "^0.1.3",
    "husky": "4.2.1",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "8.0.0",
    "lint-staged": "10.0.2",
    "prettier": "1.19.1",
    "protractor": "~5.4.0",
    "ts-node": "~8.6.0",
    "tslint": "~5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@audio/(.*)": "<rootDir>/src/app/audio/$1",
      "@shared/(.*)": "<rootDir>/src/app/shared/$1"
    },
    "globals": {
      "ts-jest": {
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
